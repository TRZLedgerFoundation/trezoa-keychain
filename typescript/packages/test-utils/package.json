{
    "name": "@trezoa/keychain-test-utils",
    "author": "TRZ Ledger Foundation",
    "license": "MIT",
    "repository": "https://github.com/trzledgerfoundation/trezoa-keychain",
    "version": "0.1.0",
    "description": "Shared testing utilities for Trezoa signer integration tests",
    "keywords": [
        "trezoa",
        "testing",
        "integration-tests",
        "trezoalitesvm"
    ],
    "type": "module",
    "sideEffects": false,
    "private": true,
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js"
        }
    },
    "files": [
        "dist",
        "src"
    ],
    "scripts": {
        "build": "tsc --build",
        "clean": "rm -rf dist *.tsbuildinfo",
        "typecheck": "tsc --noEmit"
    },
    "dependencies": {
        "@trezoa/tpl-memo": "^0.2.5",
        "@trezoa/keychain-core": "workspace:*",
        "@trezoa/kit": "^5.1.0",
        "trezoalitesvm": "^0.4.0"
    },
    "devDependencies": {
        "@types/node": "^24.10.1"
    }
}
